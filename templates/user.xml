<templateSet group="user">
  <template name="arrh" value="$kata$[] $name$ = new $kata$[$N$];" description="配列宣言する" toReformat="false" toShortenFQNames="true">
    <variable name="kata" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="N" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="toli" value=".collect(Collectors.toList());" description=".collect(Collectors.toList());" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="forre" value="for (int i = $N$ - 1; i &gt;= 0; i--) {&#10;    &#10;}" description="逆順のfori" toReformat="false" toShortenFQNames="true">
    <variable name="N" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>